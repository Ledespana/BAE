<ul>
  <% @bots.each do |bot| %>
    <div class='bot'>
      <li><%= bot.name %></li>
      <%= link_to 'Edit', edit_user_bot_path(current_user, bot), class: 'button small' %>
      <%= button_to 'Delete', user_bot_path(current_user, bot), method: :delete, class: 'button small' %>
    </div>
  <% end %>
</ul>
