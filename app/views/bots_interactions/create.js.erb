<% if @bots_interaction.persisted? %>
  var addSentence = document.querySelectorAll("#addSentence" + <%= @bots_interaction.interaction_id %>);
  var deleteSentence = document.querySelectorAll("#deleteSentence"+  <%= @bots_interaction.interaction_id %>);

  addSentence[0].classList.add("hidden");
  deleteSentence[0].classList.remove("hidden");

<% else %>
  alert("Something went wrong");
<% end %>
